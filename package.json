{
  "name": "bowboosh",
  "version": "1.0.0",
  "main": "main.min.js",
  "description": "Simple boilerplate with gulp, bower, bootstrap sass, font-awesome, bootswatch themes",
  "scripts": {
    "postinstall": "gulp install-bower && npm run build",
    "help": "gulp help",
    "start": "gulp watch --dev",
    "build": "gulp build --prod",
    "doc-dev": "gulp watch-jsdoc && gulp watch-sassdoc",
    "doc-build": "gulp build-jsdoc && gulp build-sassdoc",
    "analysis": "gulp jscpd && gulp complexity && gulp jshint",
    "report": "gulp plato-report",
    "deploy": "gulp deploySFTP",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proustibat/bowboosh.git"
  },
  "keywords": [],
  "author": "Jennifer Proust <jennifer.proust@gmail.com> (http://www.twitter.com/@proustibat)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/proustibat/bowboosh/issues"
  },
  "homepage": "https://github.com/proustibat/bowboosh",
  "devDependencies": {
    "bowboosh-tools": "^1.0.8",
    "chokidar": "^1.5.2",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-bower": "0.0.13",
    "gulp-complexity": "^0.3.2",
    "gulp-concat": "2.6.0",
    "gulp-file": "^0.3.0",
    "gulp-filter": "^4.0.0",
    "gulp-header": "^1.8.2",
    "gulp-header-license": "^1.0.6",
    "gulp-help": "^1.6.1",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.1",
    "gulp-jscpd": "0.0.7",
    "gulp-jsdoc3": "^0.3.0",
    "gulp-jshint": "^2.0.1",
    "gulp-load-plugins": "1.2.0",
    "gulp-load-subtasks": "^1.1.2",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.1",
    "gulp-sftp": "^0.1.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "1.5.3",
    "gulp-umd": "^0.2.0",
    "gulp-util": "^3.0.7",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.2.0",
    "main-bower-files": "2.13.0",
    "modernizr": "^3.3.1",
    "plato": "^1.5.0",
    "run-sequence": "^1.2.1",
    "sassdoc": "^2.1.20",
    "yargs": "^4.7.1"
  },
  "dependencies": {
    "bower": "1.7.9",
    "gulp": "3.9.1"
  },
  "private": true
}
