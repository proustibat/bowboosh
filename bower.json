{
  "name": "bowboosh",
  "description": "Simple boilerplate with gulp, bower, bootstrap sass, font-awesome, bootswatch themes",
  "version": "1.0.0",
  "ignore": [
    ".jshintrc",
    "**/*.txt",
    "**/*.iml",
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {},
  "dependencies": {
    "bootstrap-sass": "bootstrap-sass-official#3.3.6",
    "font-awesome": "fontawesome#4.6.2",
    "jquery": "1.12.3",
    "bootswatch": "3.3.6+1"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "./assets/javascripts/bootstrap.js",
        "./assets/javascripts/bootstrap/**/*.js"
      ]
    },
    "bootstrap": {
      "ignore": true
    }
  },
  "authors": [
    {
      "name": "Jennifer Proust",
      "email": "jennifer.proust@gmail.com",
      "url": "http://www.twitter.com/@proustibat"
    }
  ],
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/proustibat/bowboosh",
  "private": true
}
